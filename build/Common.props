<Project>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\build\WinSharpFuzz.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
 
  <PropertyGroup>
    <Authors>Nathaniel Bennett</Authors>
    <RepositoryUrl>https://github.com/nallixion/winsharpfuzz.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Copyright Â© 2025, Mark Burton</Copyright>
    <PackageTags>fuzzing testing fuzz-testing fuzzer libfuzzer windows</PackageTags>
  </PropertyGroup>
</Project>
